{
    "name": "underscore/underscore.php",
    "type": "library",
    "description": "Underscore.php is a PHP port of Underscore.js. This fork repackages Brian Haveri's original repo into a composer-friendly form. No changes have been made to Brian's code or tests.",
    "keywords": ["underscore", "functional"],
    "homepage": "https://github.com/adambreen/Underscore.php",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Haveri",
            "homepage": "https://github.com/brianhaveri"
        },
        {
            "name": "Adam Breen",
            "email": "adam@adambreen.com",
            "homepage": "https://github.com/adambreen"
        }

    ],
    "require": {
        "php": ">=5.3.0"        
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"        
    },
    "autoload": {
        "files": ["src/underscore.php"]        
    },
    "autoload-dev": {
        "psr-4": {
            "GuzzleHttp\\Tests\\": "tests/"
        }
    }    
}
